<h3> Here are some current available listings: </h3>

<ul id="listings">
  <% @listings.each do |listing| %>
  <li> <%=listing.address %> </li>
  <li> <%=listing.neighborhood %> </li>
  <li> <%=listing.city %> </li>
  <li> <%=number_to_currency(listing.price) %> </li>
  <li> <%=listing.home_type %> </li>
  <li> <%=listing.description %> </li>
  <hr>
  <% end %>
</ul></br>


<h4> <%= link_to "New listing", new_listing_path, id: "new_link", remote: true, :class=> "btn btn-default" %>

  <%= link_to "Edit listing", new_user_listing_path(new_user.id), id: "new_link", remote: true, :class=> "btn btn-default" %>



</h4>
